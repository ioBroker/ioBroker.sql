        "connLink":  "",
        "debounce":  1000,
        "retention": 31536000,
        "host":      "localhost",
        "port":      0,
        "user":      "",
        "password":  "",
        "dbtype":    "sqlite",
        "fileName":  "sqlite.db",
        "requestInterval": 0,
        "encrypt":   false,
        "round":     4,
        "dbname":    "iobroker",
        "multiRequests": true,
        "changesRelogInterval":      0,
        "changesMinDelta": 0
    },
    "instanceObjects": [
        {
            "_id":  "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id":  "info.connection",
            "type": "state",
            "common": {
                "role":  "indicator.connected",
                "name":  "If connected to DB",
                "type":  "boolean",
                "read":  true,
                "write": false,
                "def":   false
            },
            "native": {}
        }
    ],
    "objects": [
        {
            "_id": "_design/custom",
            "language": "javascript",
            "views": {
                "state": {
                    "map": "function(doc) { if (doc.type==='state' && (doc.common.custom || doc.common.history)) emit(doc._id, doc.common.custom || doc.common.history) }"
                }
            }
        }
    ]
