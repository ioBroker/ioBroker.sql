  "items": {
    "changesOnly": {
      "type": "checkbox",
      "label": "log changes only",
      "defaultFunc": "customObj && customObj.common && customObj.common.type === 'number'",
      "sm": 6
    },
    "debounce": {
      "type": "number",
      "label": "De-bounce time",
      "help": "De-bounce interval(ms)",
      "min": 0,
      "max": 86400000,
      "defaultFunc": "customObj && customObj.common && customObj.common.type === 'number' ? instanceObj.native.debounce : 0",
      "sm": 6
    },
    "changesRelogInterval": {
      "newLine": true,
      "type": "number",
      "label": "log changes interval(s)",
      "min": 0,
      "max": 86400000,
      "help": "0 = disabled",
      "defaultFunc": "instanceObj.native.changesRelogInterval",
      "sm": 6,
      "disabled": "!data.changesOnly"
    },
    "changesMinDelta": {
      "type": "number",
      "label": "log changes minimal delta",
      "min": 0,
      "defaultFunc": "customObj && customObj.common && customObj.common.type === 'number' ? instanceObj.native.changesMinDelta : 0",
      "default": 0,
      "sm": 6,
      "help": "0 = disable delta check",
      "disabled": "!data.changesOnly"
    },
    "storageType": {
      "newLine": true,
      "type": "select",
      "label": "Store as",
      "options": [
        {
          "value": "",
          "label": "Automatic"
        },
        {
          "value": "String",
          "label": "String"
        },
        {
          "value": "Number",
          "label": "Number"
        },
        {
          "value": "Boolean",
          "label": "Boolean"
        }
      ],
      "default": "",
      "sm": 6
    },
    "retention": {
      "type": "select",
      "label": "Storage retention",
      "options": [
        {
          "value": 0,
          "label": "keep forever"
        },
        {
          "value": 63072000,
          "label": "2 years"
        },
        {
          "value": 31536000,
          "label": "1 year"
        },
        {
          "value": 15811200,
          "label": "6 months"
        },
        {
          "value": 7948800,
          "label": "3 months"
        },
        {
          "value": 2678400,
          "label": "1 months"
        },
        {
          "value": 1209600,
          "label": "2 weeks"
        },
        {
          "value": 604800,
          "label": "1 week"
        },
        {
          "value": 432000,
          "label": "5 days"
        },
        {
          "value": 259200,
          "label": "3 days"
        },
        {
          "value": 86400,
          "label": "1 day"
        }
      ],
      "defaultFunc": "instanceObj.native.retention",
      "sm": 6
    },
    "counter": {
      "newLine": true,
      "type": "checkbox",
      "label": "Counter",
      "default": false,
      "visible": "data.storageType === 'Number'",
      "sm": 6
    },
    "aliasId": {
      "newLine": true,
      "type": "text",
      "label": "aliasId",
      "maxLength": 255,
      "default": "",
      "sm": 12,
      "help": "Alias Info"
    },
    "ignoreZero": {
      "newLine": true,
      "type": "checkbox",
      "label": "ignore zero or null values",
      "sm": 6
    },
    "ignoreBelowZero": {
      "type": "checkbox",
      "label": "ignore values below zero",
      "hidden": "!customObj || !customObj.common || customObj.common.type !== 'number'",
      "sm": 6
    }
  }
